{
	"Cria um componente": {
		"prefix": ".componente",
		"body": [
			"function ${1:componentes}() {",
			"",
			"\treturn (",
			"\t\t<>",
			"\t\t\t"
			"\t\t</>",
			"\t)",
			"}",
			"",
			"export default $0"
		],
		"description": "Cria um componente funcional"
	}




	"Componente css": {
		"prefix": ".componente-css",
		"body": [
			"import styles from ./${1:Componente}.module.css",
			"",
			"function ${1:Componente}(){",
			"",
			"\treturn (",
			"\t\t<div className={styles.$0}>",
			"\t\t\t$0",
            "\t\t<div/>",
			"\t)",
			"}",

			"export default $0"
			"",
		],
		"description": "Cria um componente funcional com estilo"
	}
}




